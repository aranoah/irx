      <div class="item">
         <div class="ui floating dropdown labeled icon button" style="height:39px;width:130px">
            <i class="filter icon"></i>

            <span class="text" style="margin-top:8px;text-align:left"> <!--ko text: showCity--><!--/ko--> </span>
            <input id="_city_" type="hidden" name="city" data-bind="textInput:city">
            <div class="menu" style="overflow-x: hidden;overflow-y: scroll;max-height: 500px;">
              <div class="ui icon search input">
                <i class="search icon"></i>
                <input type="text" name="search" placeholder="Search city...">
              </div>
              <div class="divider"></div>
              <div class="header">
                <i class="tags icon"></i>
               Select City
              </div>
              <% include city.html %> 
            </div>
          </div>
      </div>
      <div class="item">
        <div class="ui labeled input">
          <input type="hidden" data-bind="value: searchType" name="searchType">  
          <div class="ui dropdown label">
            <div class="text">Select</div>
            <i class="dropdown icon"></i>
            <div class="menu">
              <div class="item">Projects</div>
              <div class="item">Agents</div>
            </div>
          </div>
          <input type="hidden" name="projectId" placeholder="Search..." data-bind="value: projectId">
          <input type="text" id="__searchAuto" name="name" placeholder="Search..." data-bind="value: name">
        </div>
      </div>
      <div class="item">
        <div class="ui selection dropdown">
          <input type="hidden" data-bind="value: bhk" name="bhk">
          <div class="default text">BHK</div>
          <i class="angle down icon"></i>
          <div class="menu">
            <div class="item" data-value="1">1bhk</div>
            <div class="item" data-value="2">2bhk</div>
            <div class="item" data-value="3">3bhk</div>
            <div class="item" data-value="4">4bhk</div>
          </div>
        </div>
      </div>

      <div class="ui labeled icon dropdown button item">
        <i class="angle down icon"></i>
        <span class="text" id="_budget_">Budget</span>
        <div class="menu">
          <div class="header" style="background: #fff;  margin: 0;  padding: 1rem;">
            <input type="text" class="min" style="width:50px;" name="minPrice" data-bind="value:minPrice">
