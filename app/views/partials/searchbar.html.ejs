          <div class="ui icon input">
            <input type="text" placeholder="Search..." >
            <i class="search icon"></i>
          </div>
        </div>
        <div class="item filter ui dropdown link four wide column">
          Filter
          <i class="filter icon search-filter"></i>
        </div>
      </div>
    </div>
    <div class="cont-ainer search-menu computer only row">
